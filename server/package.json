{
  "name": "server",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_ENV=development ts-node-dev --no-notify --exit-child ./src/server.ts",
    "prod": "tsc && NODE_ENV=production pm2 startOrRestart pm2.config.js",
    "compile": "tsc",
    "lint": "eslint src --ext .js,.ts --fix",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "dotenv -e .env.development -- prisma migrate dev",
    "prisma:migrate:prod": "dotenv -e .env.production -- prisma migrate dev",
    "prisma:studio:dev": "dotenv -e .env.development -- prisma studio",
    "prisma:studio:prod": "dotenv -e .env.production -- prisma studio",
    "prisma:reset:dev": "dotenv -e .env.development -- prisma migrate reset dev",
    "prisma:reset:prod": "dotenv -e .env.production -- prisma migrate reset dev",
    "quotes": "ts-node ./scripts/getPassages.ts",
    "test": "dotenv -e .env.test -- jest -i"
  },
  "prisma": {
    "seed": "ts-node prisma/seed.ts"
  },
  "license": "MIT",
  "dependencies": {
    "@prisma/client": "^3.13.0",
    "@sendgrid/mail": "^7.6.1",
    "argon2": "^0.28.4",
    "axios": "^0.26.1",
    "body-parser": "^1.19.1",
    "chai": "^4.3.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "4.17.1",
    "express-rate-limit": "^6.1.0",
    "express-ws": "^5.0.2",
    "helmet": "^5.0.1",
    "hpp": "^0.2.3",
    "http-status-codes": "^2.2.0",
    "jest-mock-extended": "^2.0.4",
    "jest-websocket-mock": "^2.3.0",
    "jsonwebtoken": "^8.5.1",
    "mock-socket": "^9.1.2",
    "mockdate": "^3.0.5",
    "nodemon": "2.0.3",
    "prisma": "^3.13.0",
    "supertest": "^6.2.2",
    "uuid": "^8.3.2",
    "ws": "^8.4.2",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.3.0",
    "@types/chai-http": "^4.2.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.13",
    "@types/express-ws": "^3.0.1",
    "@types/helmet": "^4.0.0",
    "@types/hpp": "^0.2.2",
    "@types/jest": "^27.4.0",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mockdate": "^3.0.0",
    "@types/node": "^17.0.9",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "dotenv-cli": "^5.0.0",
    "eslint": "^8.7.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "^2.25.4",
    "jest": "^27.5.0",
    "jest-mock-extended": "^2.0.4",
    "pm2": "^5.1.2",
    "ts-jest": "^27.1.3",
    "ts-node": "^10.7.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.5.4"
  }
}
